<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="BackgroundOnly">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!-- <item name="android:windowNoDisplay">true</item> -->
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_agree</item>
    </style>

    <!-- 对话框title字体风格 -->
    <style name="dialogTitleStyle">
        <item name="android:textColor">#ffdeb7</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <!-- 对话框button字体风格 -->
    <style name="dialogButtonStyle">
        <item name="android:textColor">#853403</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
    </style>

    <!-- 滚动文字样式 -->
    <style name="scrollTextStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!-- 标题样式 -->
    <style name="titileStyle">

        <!-- #FFE98E -->
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#ff01264f</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <!-- 9切片背景文字样式 -->
    <style name="btnbgStyle">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#ff01264f</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <!-- 文字样式 -->
    <style name="popupTextStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!-- 基本文字样式 -->
    <style name="baseStyle">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#ff01264f</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <!-- 分区列表标题描边 -->
    <style name="gridBaseStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">normal</item>
        <item name="android:shadowColor">#1c4969</item>
        <item name="android:shadowDx">3</item>
        <item name="android:shadowDy">3</item>
        <item name="android:shadowRadius">3</item>
    </style>
    <!-- 开片推荐标题字体 -->
    <style name="cardBaseStyle">
        <item name="android:shadowColor">#ff01264f</item>
        <item name="android:shadowDx">3</item>
        <item name="android:shadowDy">3</item>
        <item name="android:shadowRadius">3</item>
        <item name="android:textColor">#ffefa5</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="contexTextStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="boldTextStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:shadowColor">#ff01264f</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="orangeTextStyle">
        <item name="android:textColor">@color/orange_btn_text_color</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="blueShareTextStyle">
        <item name="android:textColor">@color/blue_btn_text_color</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="contexTextSmallStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="contexBlackTextStyle">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <!-- 文字样式 -->
    <style name="returnTextStyle">
        <item name="android:textColor">#642a00</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="beanTextStyle">
        <item name="android:textColor">#ffdf70</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="selfTextStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="smallTextStyle" parent="baseStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="blueTextStyle">
        <item name="android:textColor">#3ca6e9</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="yellowTextStyle">
        <item name="android:textColor">#f4d806</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="midBlueTextStyle">
        <item name="android:textColor">#50b3fa</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="smallBlueTextStyle">
        <item name="android:textColor">#4480ab</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="smallYellowTextStyle">
        <item name="android:textColor">#f4d806</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="loginGiftStyle">
        <item name="android:textColor">#f4d806</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="loginGiftLabelStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="fourteenLabelStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="CenterEditStyle">
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_width">130dp</item>
        <item name="android:background">@drawable/edit_bg</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:imeOptions">actionDone</item>
    </style>

    <style name="Theme.Translucent" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/dark_disabled</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!-- 商城购买按钮文字样式 -->
    <style name="marketBuyTextStyle">
        <item name="android:textColor">@color/orange_btn_text_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>
    <!-- 透明dialog -->
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="DataSheetAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
    </style>

    <!-- 基本文字样式 -->
    <style name="pay_text">
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="Dialog_Anim">
        <item name="android:windowExitAnimation">@anim/pay_dialog_in</item>
    </style>

    <!-- 个人中心文字样式 -->
    <style name="userinfo_text">
        <item name="android:textColor">#ffdbb4</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!-- 客服中心文字样式 -->
    <style name="serverceter_text">
        <item name="android:textColor">#ffdbb4</item>
        <item name="android:textSize">15sp</item>
    </style>

    <!-- 见面有礼全屏模式 -->
    <style name="transparent_full_screen">
        <item name="android:windowBackground">@color/dialog_translucence_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>

    <style name="promotion_transparent_screen" parent="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@color/dialog_translucence_bg</item>
    </style>

    <style name="exit_1_style">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#603c05</item>
        <item name="android:background">@drawable/btn_exit_1</item>
    </style>

    <style name="exit_2_style">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#08620e</item>
        <item name="android:background">@drawable/btn_exit_2</item>
    </style>

    <style name="dialog_edit">
        <item name="android:background">@drawable/exchange_info_dialog_edit_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingLeft">5dp</item>
    </style>
    
    <!-- 推广相关的字体 -->
    <style name="promotion_text_size1">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="promotion_text_size2">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="promotion_text_size3">
        <item name="android:textSize">17sp</item>
    </style>

</resources>