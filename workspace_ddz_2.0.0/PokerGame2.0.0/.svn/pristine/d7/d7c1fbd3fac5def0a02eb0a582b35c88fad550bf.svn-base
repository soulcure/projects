package com.mykj.andr.model;

/*
 * 保存从web上解析出的数据的结构类
 */
public class ItemInfo{
	public int rankId;		// 名次
	public byte gender;	// 性别   0-表示未知 1-表示男 2-表示女
	public String nickName;	  // 玩家昵称
	public int num;  // 玩家拥有的钻石或者乐豆数量
	public int uid;
	public long guid;
	public int faceId;
	public short areaId;  // 地区编码
	public String date;
	public byte playId;
	public short vipId;
	public boolean isExpires; 
	
	public int getRankId() {
		return rankId;
	}
	public void setRankId(int rankId) {
		this.rankId = rankId;
	}
	public byte getGender() {
		return gender;
	}
	public void setGender(byte gender) {
		this.gender = gender;
	}
	public int getUid() {
		return uid;
	}
	public void setUid(int uid) {
		this.uid = uid;
	}
	public long getGuid() {
		return guid;
	}
	public void setGuid(long guid) {
		this.guid = guid;
	}
	public int getFaceId() {
		return faceId;
	}
	public void setFaceId(int faceId) {
		this.faceId = faceId;
	}
	public short getAreaId() {
		return areaId;
	}
	public void setAreaId(short areaId) {
		this.areaId = areaId;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public byte getPlayId() {
		return playId;
	}
	public void setPlayId(byte playId) {
		this.playId = playId;
	}
	public String getNickName() {
		return nickName;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	
}