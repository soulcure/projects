.charger-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
  border-bottom: solid 10rpx #f6f6f6;
}

.charger-first-item {
  border-top-style: solid;
  border-top-width: 10rpx;
  border-top-color: #f6f6f6;
}

.empty {
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;
}
